<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>Bug Tracking System</display-name>
  <servlet>
    <servlet-name>BugServlet</servlet-name>
    <servlet-class>com.chainsys.bug.controller.BugServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BugServlet</servlet-name>
    <url-pattern>/BugServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.chainsys.bug.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProjectServlet</servlet-name>
    <servlet-class>com.chainsys.bug.controller.ProjectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProjectServlet</servlet-name>
    <url-pattern>/ProjectServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TeamMemberServlet</servlet-name>
    <servlet-class>com.chainsys.bug.controller.TeamMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamMemberServlet</servlet-name>
    <url-pattern>/TeamMemberServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TeamServlet</servlet-name>
    <servlet-class>com.chainsys.bug.controller.TeamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TeamServlet</servlet-name>
    <url-pattern>/TeamServlet</url-pattern>
  </servlet-mapping>
</web-app>